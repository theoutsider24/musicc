    <xsd:simpleType name="MUSScenarioType"> <xsd:annotation><xsd:documentation xml:lang="en">Uses the descriptive terms proposed by the PEGASUS project</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Logical"><xsd:annotation><xsd:documentation xml:lang="en">Precise scenario description using technical format, but with some aspectes undefined (e.g. a vehicle may be allowed a range of speeds)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="Concrete"><xsd:annotation><xsd:documentation xml:lang="en">Fully defined description of a scenario</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSExposure">
        <xsd:annotation><xsd:documentation xml:lang="en">
            Follows ISO 26262 definitions for exposure.
            Also see WISE Drive, from University of Waterloo, Canada.
            https://uwaterloo.ca/waterloo-intelligent-systems-engineering-lab/projects/wise-drive-requirements-analysis-framework-automated-driving
        </xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="E1">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Events that occur very rarely
                </xsd:documentation></xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="E2" />
            <xsd:enumeration value="E3" />
            <xsd:enumeration value="E4">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Events that occur most of the time when operating a vehicle
                </xsd:documentation></xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSSituationDemand">
        <xsd:annotation><xsd:documentation xml:lang="en">
            Indicates the "difficulty" of the scenario.
            See WISE Drive, from University of Waterloo, Canada
            Operational Design Domain for Automated Driving Systems:
                Taxonomy of Basic Terms
            https://uwaterloo.ca/waterloo-intelligent-systems-engineering-lab/projects/wise-drive-requirements-analysis-framework-automated-driving
        </xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Low" />
            <xsd:enumeration value="Medium" />
            <xsd:enumeration value="High" />
            <xsd:enumeration value="VeryHigh" />
            <xsd:enumeration value="Extreme" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSCollisionCategory">
        <xsd:annotation><xsd:documentation xml:lang="en">
            See WISE Drive, from University of Waterloo, Canada
            Operational Design Domain for Automated Driving Systems:
                Taxonomy of Basic Terms
            https://uwaterloo.ca/waterloo-intelligent-systems-engineering-lab/projects/wise-drive-requirements-analysis-framework-automated-driving
        </xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Collision">       <xsd:annotation><xsd:documentation xml:lang="en">A crash cannot be avoided</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="NearCollision">   <xsd:annotation><xsd:documentation xml:lang="en">A crash will be imminent (at some point in the scenario) but can still be avoided</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="NormalDriving">   <xsd:annotation><xsd:documentation xml:lang="en">Any situation which is not Collision or NearCollision</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <!-- <xsd:enumeration value="Fallback" />
                Leaving out the "Fallback" category (scenarios where the ego vehicle
                is about to depart its ODD) for now, as it assumes scenarios are only
                used for a single, specified ODD -->
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSSpeedLimit">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="SlowUrban">   <xsd:annotation><xsd:documentation xml:lang="en">0  to 34.99 kmh  /  0    to 21.8 mph </xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="Urban">       <xsd:annotation><xsd:documentation xml:lang="en">35 to 59.99 kmh  /  21.8 to 37.3 mph </xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="FastUrban">   <xsd:annotation><xsd:documentation xml:lang="en">60 to 74.99 kmh  /  37.3 to 46.6 mph </xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="TrunkRoad">   <xsd:annotation><xsd:documentation xml:lang="en">75 to 99.99 kmh  /  46.6 to 62.1 mph </xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="Highway">     <xsd:annotation><xsd:documentation xml:lang="en">100+ kmh         /  62.1+ mph </xsd:documentation></xsd:annotation> </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSADASFeature">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ABS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Anti-lock Braking System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="ACC">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Adaptive Cruise Control
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="AEB">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Autonomous Emergency Braking
                    Low Speed Collision Avoidance Systems
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="AFL">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Adaptive Front Lights
                    also known as: AFLS, Advanced Front Lighting Systems
                                   Adaptive Headlights
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="ALKS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Automatic lane keeping system (designed for 'hands off' use)
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="BSM">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Blind Spot Monitoring
                    also known as: BSW, Blind Spot Warning
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="CSW">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Curve Speed Warning
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="CWS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Collision Warning System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="DMS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Driver Monitoring System
                    also known as: Drowsiness Alert
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="DPP">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Dynamic Pass Predictor
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="ESC">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Electronic Stability Control
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="FCW">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Forward Collision Warning
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HUD">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Heads Up Display
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="ISA">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Intelligent Speed Adaptation
                    also known as: Intelligent Speed Assist
                                   HSA, High Speed Alert
                </xsd:documentation></xsd:annotation></xsd:enumeration>
<!--        <xsd:enumeration value="LCA">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Lane Change Assist
                      - not included, as sub-types LKA and LDW are more specific
                </xsd:documentation></xsd:annotation></xsd:enumeration> -->
            <xsd:enumeration value="LDW">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Lane Departure Warning
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="LKA">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Lane Keep Assist
                    also known as: LKAS, Lane Keep Assist System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="NVS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Night Vision System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PA">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Park Assist
                    also known as: IPA, Intelligent Park Assist
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="PDS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Pedestrian Detection System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="TPMS">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Tire Pressure Monitoring System
                </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="TSR">
                <xsd:annotation><xsd:documentation xml:lang="en">
                    Traffic Sign Recognition
                    also known as: RSR, Road Sign Recognition
                </xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSModelDetailLevel">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BoundingBox" > <xsd:annotation><xsd:documentation xml:lang="en">Scenery file provides bounding boxes for objects only. Suitable for providing restricted sightlines only</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="LowPoly" > <xsd:annotation><xsd:documentation xml:lang="en">Majority of objects use (or may conditionally use) a relatively small number of polygons. Suitable for real time or faster rendering</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HighPoly"> <xsd:annotation><xsd:documentation xml:lang="en">Majority of objects use (or may conditionally use) a relatively large number of polygons. May not be suitable for real time or faster rendering</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSTargetSensors">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="CameraRealistic"> <xsd:annotation><xsd:documentation xml:lang="en">Suitable for use in perception tests of camera based systems</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="LidarRealistic"> <xsd:annotation><xsd:documentation xml:lang="en">Suitable for use in perception tests of LIDAR based systems</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="RadarRealistic"> <xsd:annotation><xsd:documentation xml:lang="en">Suitable for use in perception tests of RADAR based systems</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSRoadFeature">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Roundabout" />
            <xsd:enumeration value="HighwayEntranceRamp" > <xsd:annotation><xsd:documentation xml:lang="en">AKA slip road, on-ramp</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HighwayExitRamp" > <xsd:annotation><xsd:documentation xml:lang="en">AKA slip road, off-ramp</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="TrafficLightControlledJunction" />
            <xsd:enumeration value="3LegJunction" > <xsd:annotation><xsd:documentation xml:lang="en">Often a T-Junction</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="4LegJunction" />
            <xsd:enumeration value="MoreThan4LegJunction" />

            <xsd:enumeration value="RailwayCrossing" />
            <xsd:enumeration value="PedestrianCrossing" />

            <xsd:enumeration value="StopSign" />
            <xsd:enumeration value="GiveWaySign" >  <xsd:annotation><xsd:documentation xml:lang="en">AKA yield sign</xsd:documentation></xsd:annotation></xsd:enumeration>

            <xsd:enumeration value="1Lane" >  <xsd:annotation><xsd:documentation xml:lang="en">Number of lanes in the direction of travel. Include shared lanes (e.g. central overtaking lane with equal priority)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="2Lane" />
            <xsd:enumeration value="3Lane" />
            <xsd:enumeration value="4Lane" />
            <xsd:enumeration value="MoreThan4Lanes" />
            <xsd:enumeration value="CentralLaneSharedWithOncomingTraffic"> <xsd:annotation><xsd:documentation xml:lang="en">One or more lanes is shared with oncoming traffic (e.g. single track road, rural road with central overtaking lane available to both directions). Does not include tidal flow lanes controlled by signals</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="DividedCarriageway" >  <xsd:annotation><xsd:documentation xml:lang="en">Implies a physical median, AKA a central reservation</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="OneWayRoad" />
            <xsd:enumeration value="BicycleLane" />

            <xsd:enumeration value="HardShoulderPresent"> <xsd:annotation><xsd:documentation xml:lane="en">Wide enough for normal size vehicles to stop without any part being in a running lane</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="HighGradient" />
            <xsd:enumeration value="SharpBend" />
            <xsd:enumeration value="Roadworks" />
            <xsd:enumeration value="Tunnel" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSEnvironmentalCondition">
        <xsd:restriction base="xsd:string">
            <!-- Weather -->
            <xsd:enumeration value="Rain" />
            <xsd:enumeration value="NoRainButRoadWet" />
            <xsd:enumeration value="Snow" />
            <xsd:enumeration value="Ice" />
            <xsd:enumeration value="Fog" />
            <xsd:enumeration value="Cloudy" />
            <!-- Time-of-day -->
            <xsd:enumeration value="Daylight" />
            <xsd:enumeration value="Night" />
            <xsd:enumeration value="Dawn" />
            <xsd:enumeration value="Dusk" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSVehicleType">
        <xsd:annotation><xsd:documentation xml:lang="en">Type codes from UNECE Consolidated Resolution on the Construction of Vehicles (R.E.3), Revision 6, plus a few MUSICC-specific extensions</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="L1" > <xsd:annotation><xsd:documentation xml:lang="en">[Moped] A two-wheeled vehicle with an engine cylinder capacity in the case of a thermic engine not exceeding 50 cm³ and whatever the means of propulsion a maximum design speed not exceeding 50 km/h.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L2" > <xsd:annotation><xsd:documentation xml:lang="en">[Low power tricycle] A three-wheeled vehicle of any wheel arrangement with an engine cylinder capacity in the case of a thermic engine not exceeding 50 cm³ and whatever the means of propulsion a maximum design speed not exceeding 50 km/h.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L3" > <xsd:annotation><xsd:documentation xml:lang="en">[Motorbike] A two-wheeled vehicle with an engine cylinder capacity in the case of a thermic engine exceeding 50 cm³ or whatever the means of propulsion a maximum design speed exceeding 50 km/h.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L4" > <xsd:annotation><xsd:documentation xml:lang="en">[Motorbike with side car] A vehicle with three wheels asymmetrically arranged in relation to the longitudinal median plane with an engine cylinder capacity in the case of a thermic engine exceeding 50 cm³ or whatever the means of propulsion a maximum design speed exceeding 50 km/h.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L5" > <xsd:annotation><xsd:documentation xml:lang="en">[Fast Tricycle] A vehicle with three wheels symmetrically arranged in relation to the longitudinal median plane with an engine cylinder capacity in the case of a thermic engine exceeding 50 cm³ or whatever the means of propulsion a maximum design speed exceeding 50 km/h.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L6" > <xsd:annotation><xsd:documentation xml:lang="en">[Light quadricycle] A vehicle with four wheels whose unladen mass is not more than 350 kg, not including the mass of the batteries in case of electric vehicles, whose maximum design speed is not more than 45 km/h, and whose engine cylinder capacity does not exceed 50 cm³ for spark (positive) ignition engines, or whose maximum net power output does not exceed 4 kW in the case of other internal combustion engines, or whose maximum continuous rated power does not exceed 4 kW in the case of electric engines.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="L7" > <xsd:annotation><xsd:documentation xml:lang="en">[Heavy quadricycle] A vehicle with four wheels, other than that classified for the category L6, whose unladen mass is not more than 400 kg (550 kg for vehicles intended for carrying goods), not including the mass of batteries in the case of electric vehicles and whose maximum continuous rated power does not exceed 15 kW.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="M1" > <xsd:annotation><xsd:documentation xml:lang="en">[Car] Vehicles used for carriage of passengers, comprising not more than eight seats in addition to the driver's = 9.(up to eg: London Cab / E7 Type Vehicle 8 seat + Driver.)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="M2" > <xsd:annotation><xsd:documentation xml:lang="en">[Minibus] Vehicles used for the carriage of passengers, comprising more than eight seats in addition to the driver's seat, and having a maximum mass not exceeding 5 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="M3" > <xsd:annotation><xsd:documentation xml:lang="en">[Bus] Vehicles used for the carriage of passengers, comprising more than eight seats in addition to the driver's seat, and having a maximum mass exceeding 5 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="N1" > <xsd:annotation><xsd:documentation xml:lang="en">[Pick-up Truck, Van] Vehicles used for the carriage of goods and having a maximum mass not exceeding 3.5 tonnes. </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="N2" > <xsd:annotation><xsd:documentation xml:lang="en">[Small Truck] used for the carriage of goods and having a maximum mass exceeding 3.5 tonnes but not exceeding 12 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="N3" > <xsd:annotation><xsd:documentation xml:lang="en">[Large truck] Vehicles used for the carriage of goods and having a maximum mass exceeding 12 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="O1" > <xsd:annotation><xsd:documentation xml:lang="en">Trailers with a maximum mass not exceeding 0.75 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="O2" > <xsd:annotation><xsd:documentation xml:lang="en">Trailers with a maximum mass exceeding 0.75 tonnes, but not exceeding 3.5 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="O3" > <xsd:annotation><xsd:documentation xml:lang="en">Trailers with a maximum mass exceeding 3.5 tonnes, but not exceeding 10 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="O4" > <xsd:annotation><xsd:documentation xml:lang="en">Trailers with a maximum mass exceeding 10 tonnes.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="T" > <xsd:annotation><xsd:documentation xml:lang="en">Agricultural and Forestry tractor</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="G" > <xsd:annotation><xsd:documentation xml:lang="en">Off-road vehicles</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SA" > <xsd:annotation><xsd:documentation xml:lang="en">Motor caravan</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SB" > <xsd:annotation><xsd:documentation xml:lang="en">Armoured vehicle</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SC" > <xsd:annotation><xsd:documentation xml:lang="en">Ambulance</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SD" > <xsd:annotation><xsd:documentation xml:lang="en">Hearse</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="NRMM" > <xsd:annotation><xsd:documentation xml:lang="en">Non-road mobile machinery</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="SPV" > <xsd:annotation><xsd:documentation xml:lang="en">Special purpose vehicle. A vehicle of category M, N or O for conveying passengers or goods and for performing a special function for which special body arrangements and/or equipment are necessary.</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="POD"> <xsd:annotation><xsd:documentation xml:lang="en">Low-speed passenger carrying pod</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSEgoManeuverType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="RightTurn" />
            <xsd:enumeration value="LeftTurn" />
            <xsd:enumeration value="GoStraightAtJunction" />
            <xsd:enumeration value="CrossTrafficTurn" > <xsd:annotation><xsd:documentation xml:lang="en">Left turn in countries that drive on the right</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="WithTrafficTurn" > <xsd:annotation><xsd:documentation xml:lang="en">Right turn in countries that drive on the right</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="UTurn" />
            <xsd:enumeration value="TurnOntoMinorRoad" />
            <xsd:enumeration value="TurnOntoMajorRoad" />
            <xsd:enumeration value="HighwayMerge" />
            <xsd:enumeration value="HighwayExit" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSKeyActorType">
        <xsd:annotation><xsd:documentation xml:lang="en">
            See WISE Drive, from University of Waterloo, Canada
            Operational World Model Ontology for Automated Driving Systems
                Part 2: Road Users, Animals, Other Obstacles, and Environmental Conditions
            https://uwaterloo.ca/waterloo-intelligent-systems-engineering-lab/projects/wise-drive-requirements-analysis-framework-automated-driving
        </xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="PassengerCar" />
            <xsd:enumeration value="Truck" />
            <xsd:enumeration value="Bus" />
            <xsd:enumeration value="Motorcycle" />
            <xsd:enumeration value="SmallLowSpeedVehicle" />
            <xsd:enumeration value="EmergencyVehicle" />
            <xsd:enumeration value="PedalCycle" />
            <xsd:enumeration value="Trailer" />
            <xsd:enumeration value="MotorizedOffRoadVehicle" />
            <xsd:enumeration value="AnimalDrawnVehicle" />
            <xsd:enumeration value="RailedVehicle" />
            <xsd:enumeration value="RailRoadVehicle" />
            <xsd:enumeration value="AnimalRider" />
            <xsd:enumeration value="Pedestrian" />
            <xsd:enumeration value="TrafficControlPerson" />
            <!-- Extra categories not part of WISE Drive -->
            <xsd:enumeration value="Animal" />
            <xsd:enumeration value="InanimateObstacle" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSKeyActorAction">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ActorCrossingRoad" />
            <xsd:enumeration value="ActorStoppedInRoad" />
            <xsd:enumeration value="EmergencyStop" />
            <xsd:enumeration value="CutIn" />
            <xsd:enumeration value="Overtake" />
            <xsd:enumeration value="WrongWayTravel" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSRuleSet">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="RSSReasonableBehaviour"> <xsd:annotation><xsd:documentation xml:lang="en">An actor does not comply with 'reasonable beahviour' as defined by by Intel Mobileye Responsibility Sensitive Safety</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="NationalAdvice"> <xsd:annotation><xsd:documentation xml:lang="en">An actor does not comply with official advice to drivers in the country of the scenario e.g. the UK highway code</xsd:documentation></xsd:annotation> </xsd:enumeration>
            <xsd:enumeration value="NationalLaws"> <xsd:annotation><xsd:documentation xml:lang="en">An actor breaks a specific, well defined, law (e.g. breaking the speed limit, but not careless driving)</xsd:documentation></xsd:annotation> </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSTrafficDensity">
        <xsd:annotation><xsd:documentation xml:lang="en">
            The intention of the traffic density is to represent the maximum density section
            encountered by the ego vehicle during the scenario.
            https://en.wikipedia.org/wiki/Traffic_flow#Overview
        </xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="EgoOnly">       <xsd:annotation><xsd:documentation xml:lang="en">Scenario only contains the ego vehicle, no other vehicles</xsd:documentation></xsd:annotation></xsd:enumeration>            
            <xsd:enumeration value="Low" >          <xsd:annotation><xsd:documentation xml:lang="en">one or two other vehicles in scenario </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="FreeFlow" >     <xsd:annotation><xsd:documentation xml:lang="en">2 to 11.99 vehicles/mile/lane </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="MaxStable" >    <xsd:annotation><xsd:documentation xml:lang="en">12 to 29.99 vehicles/mile/lane </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="Unstable" >     <xsd:annotation><xsd:documentation xml:lang="en">30 to 64.99 vehicles/mile/lane </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="StopStart" >    <xsd:annotation><xsd:documentation xml:lang="en">65 to 150 vehicles/mile/lane </xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="Jam" >          <xsd:annotation><xsd:documentation xml:lang="en">150+ vehicles/mile/lane </xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="MUSTrafficAverageSpeed">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Stationary" >       <xsd:annotation><xsd:documentation xml:lang="en">Strictly zero kmh</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="Walking" >          <xsd:annotation><xsd:documentation xml:lang="en">0.000001 to 6.99 kmh</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="FastTrafficJam" >   <xsd:annotation><xsd:documentation xml:lang="en"> 7 kmh to 29.99 kmh  (30 kph = 19 mph</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="UrbanFlowing" >     <xsd:annotation><xsd:documentation xml:lang="en">30 kmh to 59.99 kmh  (60 kph = 37 mph)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="TrunkFlowing" >     <xsd:annotation><xsd:documentation xml:lang="en">60 kmh to 99.99 kmh  (100 kph = 62 mph)</xsd:documentation></xsd:annotation></xsd:enumeration>
            <xsd:enumeration value="HighwayFlowing" >   <xsd:annotation><xsd:documentation xml:lang="en">100+ kmh</xsd:documentation></xsd:annotation></xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="MUSEntityUnderTest">
        <xsd:sequence>
            <xsd:element name="EntityRef">
                <xsd:complexType>
                    <xsd:attribute name="entityRef" type="xsd:string" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="GoalPosition" type="MUSPosition"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--This makes use of types defined in the OpenSCENARIO schema -->
      <xsd:complexType name="MUSPosition">
        <xsd:complexContent>
            <xsd:restriction base="Position">
                <xsd:choice>
                      <xsd:element name="WorldPosition" type="WorldPosition" minOccurs="0"/>
                      <xsd:element name="RoadPosition" type="RoadPosition" minOccurs="0"/>
                      <xsd:element name="LanePosition" type="LanePosition" minOccurs="0"/>
                </xsd:choice>
            </xsd:restriction>
        </xsd:complexContent>
      </xsd:complexType>

    <!-- Section aligned with PEGASUS logical scenario format, for partial compatability -->
    <xsd:complexType name="MUSParameterStochastics">
        <xsd:annotation><xsd:documentation xml:lang="en">
            This type is closely aligned with the PEGASUS format for logical scenarios,
            and the "parameterSpace" element from their XML schema. However, parameter 
			dependencies are not supported, and a smaller range of distribution types are
			supported, meaning there is only partial compatability with the PEGASUS schema.
        </xsd:documentation></xsd:annotation>
        <xsd:all>
            <xsd:element name ="RoadNetwork">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ParameterGroup" type="MUSParameterGroup" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name ="OpenScenario">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ParameterGroup" type="MUSParameterGroup" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="MUSParameterGroup">
        <xsd:sequence>
            <xsd:element name="Parameter" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="Distribution">
                            <xsd:complexType>
                                <xsd:all>
                                    <xsd:element ref="Choice_Distribution"/>
                                </xsd:all>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                    <xsd:attribute name="name" type="xsd:string" use="required"/>
                    <xsd:attribute name="unit" type="xsd:string" use="required"/>
                    <xsd:attribute name="type" type="enum_type" use="required"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:element name="Choice_Distribution" abstract="true"/>
    <xsd:element name="UniformDistribution" substitutionGroup="Choice_Distribution">
        <xsd:complexType>
            <xsd:attribute name="lowerLimit" type="xsd:double" use="required"/>
            <xsd:attribute name="upperLimit" type="xsd:double" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NormalDistribution" substitutionGroup="Choice_Distribution">
        <xsd:complexType>
            <xsd:attribute name="expectedValue" type="xsd:double" use="required"/>
            <xsd:attribute name="variance" type="xsd:double" use="required"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SingleValues" substitutionGroup="Choice_Distribution">
        <xsd:complexType>
            <xsd:all>
                <xsd:element ref="Choice_Sequence_List"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="Choice_Sequence_List" abstract="true"/>

    <xsd:element name="List" substitutionGroup="Choice_Sequence_List">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="ListElement" minOccurs="1" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:attribute name="value" type="xsd:anySimpleType" use="required"/>
                        <xsd:attribute name="label" type="xsd:string" use="optional"/>
                        <xsd:attribute name="probabilityOfOccurence" use="optional">
                            <xsd:annotation><xsd:documentation xml:lang="en">Sum of probabilities should be less than or equal to 1. If  less than 1, remaining probability space will be shared equally between list items without a specified probability. If none exist, it will be allocated to default parameter value.</xsd:documentation></xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:decimal">
                                    <xsd:minInclusive value="0"/>
                                    <xsd:maxInclusive value="1"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:simpleType name="enum_type">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="string" />
            <xsd:enumeration value="double" />
            <xsd:enumeration value="int" />
        </xsd:restriction>
    </xsd:simpleType>
    <!-- End of section aligned with PEGASUS logical scenario format -->

</xsd:schema>
