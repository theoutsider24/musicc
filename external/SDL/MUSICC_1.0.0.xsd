<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:import namespace="http://www.w3.org/XML/1998/namespace"
                schemaLocation="xml.xsd"/>
    <xsd:include schemaLocation="OpenSCENARIO.xsd"/>

    <xsd:element name="MUSICCScenario">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="FileHeader"             type="MUSFileHeader"/>
                <xsd:element name="Metadata"               type="MUSMetadata"/>
                <xsd:element name="EntityUnderTest"        type="MUSEntityUnderTest"      minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="ParameterStochastics"   type="MUSParameterStochastics" minOccurs="0"/>
                
                <xsd:element name="OpenSCENARIO">
                    <xsd:complexType>
                        <xsd:attribute name="filepath" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element name="OpenDRIVE">
                    <xsd:complexType>
                        <xsd:attribute name="filepath" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element>

                <!--for future implementation. For now, loggers and tests will be stored in the resources directory and are defined by their file name-->
                <!--xsd:element name="Loggers>
                    <xsd:complexType>
                        <xsd:attribute name="filepath" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element name="Tests">
                    <xsd:complexType>
                        <xsd:attribute name="filepath" type="xsd:string" use="required"/>
                    </xsd:complexType>
                </xsd:element-->
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="MUSFileHeader">
        <xsd:annotation><xsd:documentation xml:lang="en">
            The label and version of a MUSICC scenario form a unique key, and correspond
            to a unique MUSICC scenario ID. The ID is conventionally used as the filename,
            in the form "ID.xmus"
        </xsd:documentation></xsd:annotation>

        <!-- Revision number represented by THIS XSD FILE is fixed -->
        <xsd:attribute name="revision"        type="xsd:string"          use="required"      fixed="1.0.0"/>
        <xsd:attribute name="label"                                      use="required">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:maxLength value="50"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="version"         type="xsd:positiveInteger" use="required"/>
        <xsd:attribute name="updateDateTime"  type="xsd:dateTime"        use="required"/>
        <xsd:attribute name="updateUsername"  type="xsd:string"          use="required"/>
    </xsd:complexType>

    <xsd:complexType name="MUSMetadata">
        <xsd:sequence>

            <xsd:element name="Description" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:maxLength value="2048"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="CountryCode" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSISOCountryCode" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="DriveOnRightOrLeft" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="Right" />
                                <xsd:enumeration value="Left" />
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="UseCase" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSUseCase" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="ScenarioType" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSScenarioType" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="Exposure" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSExposure" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="TestcaseWeighting" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:minInclusive value = "0"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="SituationDemand" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSSituationDemand" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="CollisionCategory" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSCollisionCategory" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="InitialSpeedLimit" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSSpeedLimit" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RealWorldMap" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="xsd:boolean" use="required"/>
                    <xsd:attribute name="mapDataValidDateTime" type="xsd:dateTime" use="optional"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RealWorldCoordinate" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="latitude" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:minInclusive value="-90"/>
                                <xsd:maxInclusive value="90"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                    <xsd:attribute name="longitude" use="required">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:decimal">
                                <xsd:minInclusive value="-180"/>
                                <xsd:maxInclusive value="180"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RepresentsADASTest" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="xsd:boolean" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="ADASFeaturesTested" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ADASFeature" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSADASFeature" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="SceneGraphModelDetail" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSModelDetailLevel" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="SceneGraphModelSensorRealism" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TargetSensor" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSTargetSensors" use="required"/> 
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RoadFeatures" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="RoadFeature" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSRoadFeature" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="EnvironmentalConditions" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="EnvironmentalCondition" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSEnvironmentalCondition" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="IntendedEgoVehicleTypes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="EgoVehicleType" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSVehicleType" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="EgoManeuverTypes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="EgoManeuverType" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSEgoManeuverType" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="KeyActorTypes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="KeyActorType" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSKeyActorType" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="KeyActorActions" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="KeyActorAction" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSKeyActorAction" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="KeyActorRulesBroken" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="RuleSet" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="MUSRuleSet" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="TrafficDensity" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSTrafficDensity" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="TrafficAverageSpeed" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="MUSTrafficAverageSpeed" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="GlobalTags" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="GlobalTag" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="xsd:string" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="Regulations" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Regulation" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="value" type="xsd:string" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="CustomMetadata" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="CustomTag" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="name"  type="xsd:string" use="required"/>
                                <xsd:attribute name="value" type="xsd:string" use="required"/>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="SourceOrganization" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="xsd:string" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="SourceFilepath" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="xsd:string" use="required"/>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="OwningOrganization" minOccurs="0">
                <xsd:complexType>
                    <xsd:attribute name="value" type="xsd:string" use="required"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

